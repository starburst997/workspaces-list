{
  "name": "workspaces-list",
  "displayName": "Workspaces List",
  "description": "Manage multiple VSCode/Cursor workspaces with Claude Code status monitoring",
  "version": "0.1.0",
  "publisher": "your-publisher-name",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "workspace",
    "window manager",
    "claude-code",
    "productivity"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "workspacesList",
          "name": "Workspaces",
          "order": -1
        }
      ]
    },
    "commands": [
      {
        "command": "workspacesList.refresh",
        "title": "Refresh Workspaces",
        "icon": "$(refresh)"
      },
      {
        "command": "workspacesList.focusWorkspace",
        "title": "Focus Workspace"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "workspacesList.refresh",
          "when": "view == workspacesList",
          "group": "navigation"
        }
      ]
    },
    "colors": [
      {
        "id": "workspacesList.claudeOrange",
        "description": "Claude Code executing status color (orange)",
        "defaults": {
          "dark": "#D97706",
          "light": "#D97706",
          "highContrast": "#D97706",
          "highContrastLight": "#D97706"
        }
      },
      {
        "id": "workspacesList.gradientGreen",
        "description": "Recently finished gradient - green (0 min)",
        "defaults": {
          "dark": "#4EC9B0",
          "light": "#4EC9B0",
          "highContrast": "#4EC9B0",
          "highContrastLight": "#4EC9B0"
        }
      },
      {
        "id": "workspacesList.gradientCyan1",
        "description": "Recently finished gradient - cyan 1 (~5 min)",
        "defaults": {
          "dark": "#3BADB8",
          "light": "#3BADB8",
          "highContrast": "#3BADB8",
          "highContrastLight": "#3BADB8"
        }
      },
      {
        "id": "workspacesList.gradientCyan2",
        "description": "Recently finished gradient - cyan 2 (~10 min)",
        "defaults": {
          "dark": "#2891BF",
          "light": "#2891BF",
          "highContrast": "#2891BF",
          "highContrastLight": "#2891BF"
        }
      },
      {
        "id": "workspacesList.gradientCyan3",
        "description": "Recently finished gradient - cyan 3 (~15 min)",
        "defaults": {
          "dark": "#1574C7",
          "light": "#1574C7",
          "highContrast": "#1574C7",
          "highContrastLight": "#1574C7"
        }
      },
      {
        "id": "workspacesList.gradientBlue1",
        "description": "Recently finished gradient - blue 1 (~20 min)",
        "defaults": {
          "dark": "#0258CE",
          "light": "#0258CE",
          "highContrast": "#0258CE",
          "highContrastLight": "#0258CE"
        }
      },
      {
        "id": "workspacesList.gradientBlue2",
        "description": "Recently finished gradient - blue 2 (~25 min)",
        "defaults": {
          "dark": "#013CCB",
          "light": "#013CCB",
          "highContrast": "#013CCB",
          "highContrastLight": "#013CCB"
        }
      },
      {
        "id": "workspacesList.gradientBlue3",
        "description": "Recently finished gradient - blue 3 (30 min)",
        "defaults": {
          "dark": "#007ACC",
          "light": "#007ACC",
          "highContrast": "#007ACC",
          "highContrastLight": "#007ACC"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint src --ext ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^20.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.x",
    "@typescript-eslint/parser": "^6.x",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.x",
    "prettier": "^3.x",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "lucide": "^0.294.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/workspaces-list"
  },
  "os": [
    "darwin"
  ]
}
